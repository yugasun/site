{
  "private": true,
  "version": "0.0.1",
  "name": "serverless-site",
  "homepage": "https://serverless.com",
  "repository": "https://github.com/serverless/site.git",
  "phenomic": {
    "CNAME": true,
    "cache": true,
    "offline": false
  },
  "scripts": {
    "lintfix": "eslint --ignore-path .gitignore --fix .",
    "lint:js": "eslint --ignore-path .gitignore src",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "prestart": "npm run sync",
    "start": "phenomic start",
    "refresh": "npm run refreshDocs && npm run refreshBlog",
    "prerefreshDocs": "rimraf node_modules/serverless && rimraf node_modules/serverless",
    "refreshDocs": "node scripts/docs/pullLatestDocs.js",
    "prerefreshBlog": "rimraf node_modules/serverless-blog && rimraf node_modules/serverless-blog",
    "refreshBlog": "node scripts/blog/pullLatestBlog.js",
    "sync": "node scripts/docs/entry.js && node scripts/blog/entry.js",
    "prebuild": "npm run sync",
    "build": "phenomic build",
    "buildprod": "npm run refresh && npm run sync && phenomic build",
    "postinstall": "node scripts/build/postinstall.js && npm run refresh",
    "postbuild": "node scripts/redirects.js",
    "static": "cd dist && http-server",
    "pretest": "npm run lint",
    "build-components": "webpack --config webpack.config.components.babel.js && node scripts/components/post-build.js",
    "dll": "webpack --display-chunks --color --config webpack.DLL.babel.js"
  },
  "devDependencies": {
    "async": "^2.0.1",
    "auth0-lock": "10.2.1",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.0-beta.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.3.13",
    "cheerio": "^0.22.0",
    "classnames": "^2.2.3",
    "color": "^0.11.1",
    "css-loader": "^0.23.0",
    "cssnano": "^3.7.7",
    "directory-tree": "^1.1.0",
    "download": "^5.0.2",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.4.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "git://github.com/ServerlessInc/extract-text-webpack-plugin.git#webpack-1",
    "file-loader": "^0.8.1",
    "fs-extra": "^0.30.0",
    "gray-matter": "^2.0.2",
    "history": "^2.0.0",
    "htmlparser2": "^3.9.1",
    "invariant": "^2.1.1",
    "jade": "^1.11.0",
    "json-loader": "^0.5.2",
    "mv": "^2.1.1",
    "node-dir": "^0.1.16",
    "npm-run-all": "^1.7.0",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.7.0",
    "postcss-math": "0.0.5",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.3.0",
    "postcss-simple-vars": "^3.0.0",
    "prepend-file": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react-redux": "^4.4.1",
    "react-router": "^2.3.0",
    "react-svg-inline": "^1.1.0",
    "redux": "^3.0.0",
    "rimraf": "^2.5.4",
    "sitemap": "^1.8.1",
    "style-loader": "^0.12.3",
    "stylelint": "^6.8.0",
    "stylelint-config-standard": "^10.0.0",
    "svgo": "^0.7.0",
    "webpack": "^1.12.1",
    "whatwg-fetch": "^0.11.0",
    "yamljs": "^0.2.8"
  },
  "babel": {
    "presets": [
      "babel-preset-react",
      "babel-preset-es2015",
      "babel-preset-stage-1"
    ],
    "env": {
      "production": {
        "presets": [
          "babel-preset-react-optimize"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint-config-i-am-meticulous/react"
    ],
    "rules": {
      "react/prefer-stateless-function": 0
    }
  },
  "eslintConfigRuleReact/prefer-stateless-function": "https://github.com/MoOx/phenomic/issues/46",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "block-no-empty": null
    }
  },
  "dependencies": {
    "auth0-lock": "10.2.1",
    "axios": "^0.13.1",
    "classnames": "^2.2.5",
    "jwt-decode": "^2.1.0",
    "phenomic": "0.17.8",
    "react": "^15.0.0-rc.1",
    "react-auto-form": "^1.3.0",
    "react-dom": "^15.0.0-rc.1",
    "react-google-analytics": "^0.2.0",
    "react-helmet": "^3.0.0",
    "redux-thunk": "^2.1.0"
  },
  "dllPlugin": {
    "path": "node_modules/@dummy-dll-folder"
  }
}
