{
  "name": "@serverless/site",
  "private": true,
  "description": "Serverless Website",
  "version": "1.0.0",
  "dependencies": {
    "algoliasearch": "^3.30.0",
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.6",
    "clipboard": "^2.0.1",
    "dotenv": "^6.0.0",
    "eslint": "^5.3.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "gatsby": "^1.9.277",
    "gatsby-link": "^1.6.46",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-feed": "^1.3.19-14",
    "gatsby-plugin-feed-generator": "^1.0.6",
    "gatsby-plugin-google-tagmanager": "^2.0.5",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sitemap": "^1.2.14-12",
    "gatsby-plugin-styled-components": "^2.0.11",
    "localforage": "^1.7.2",
    "lodash": "^4.17.11",
    "rc-collapse": "^1.11.1",
    "react-device-detect": "^1.6.2",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-iframe": "^1.3.3",
    "react-instantsearch-dom": "^5.2.3",
    "react-metismenu": "^1.4.0",
    "react-modal": "^3.8.1",
    "react-player": "^1.9.1",
    "react-responsive": "^6.0.1",
    "react-slick": "^0.24.0",
    "react-text-loop": "^2.0.1",
    "react-tooltip": "^3.10.0",
    "remark-autolink-headings": "^5.0.0",
    "remark-highlight.js": "^5.0.0",
    "remark-slug": "^4.2.3",
    "serverless-design-system": "0.0.2-p2",
    "slick-carousel": "^1.8.1",
    "styled-components": "^3.3.3",
    "styled-system": "3.1.6",
    "sync-request": "^4.1.0",
    "url": "^0.11.0",
    "url-loader": "^1.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build && mv public dist && cp src/assets-root/* dist",
    "buildprod": "npm run syncProd && npm run prepareContent && gatsby build && mv public dist && cp src/assets-root/* dist",
    "buildprodlocal": "export NODE_OPTIONS=--max_old_space_size=4096 && npm run buildprod",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint ./src/ --ext .js --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:blog": "node scripts/blog/watch.js",
    "components": "npm run transpile -- --watch",
    "prepareContent": "node scripts/docs && node scripts/blog && node scripts/example && node scripts/plugins && node scripts/build-config && node scripts/docs/generate-new-menu.js",
    "prepareContentCn": "node scripts/cn/components && node scripts/cn/caseStudies",
    "dev": "npm run syncProd && npm run syncProdCn && npm run prepareContent && npm run prepareContentCn && gatsby develop",
    "syncLocal": "npm run postinstall",
    "syncProd": "npm run syncProd:Blog && npm run syncProd:Docs && npm run syncProd:Examples && npm run syncProd:Jobs && npm run syncProd:Plugins",
    "syncProd:Blog": "node scripts/blog/sync-blog-remote.js",
    "syncProd:Docs": "node scripts/docs/sync-docs-remote.js",
    "syncProd:Examples": "node scripts/example/sync-examples-remote.js",
    "syncProd:Plugins": "node scripts/plugins/sync-plugins-remote.js",
    "syncProd:Jobs": "node scripts/jobs/sync-jobs-remote.js",
    "syncProdCn": "npm run syncProd:CnComponents && npm run syncProd:CnCaseStudies",
    "syncProd:CnComponents": "node scripts/cn/components/sync-components-remote.js",
    "syncProd:CnCaseStudies": "node scripts/cn/caseStudies/sync-caseStudies-remote.js",
    "prebuild": "npm run prepareContent",
    "postbuildprod": "node scripts/redirects.js",
    "postbuild": "node scripts/redirects.js"
  },
  "devDependencies": {
    "async": "^2.6.1",
    "download": "^7.1.0",
    "fs-extra": "^7.0.0",
    "gatsby-plugin-root-import": "^1.0.0",
    "git-branch": "^2.0.1",
    "gray-matter": "^4.0.1",
    "node-dir": "^0.1.17",
    "prettier": "^1.13.7",
    "react-masonry-css": "^1.0.11",
    "remark-html": "^8.0.0",
    "remark-parse": "^5.0.0",
    "unified": "^7.0.0",
    "yamljs": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
